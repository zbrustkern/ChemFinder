<h1>Listing chemicals</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Casnum</th>
    <th>Location</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @chemicals.each do |chemical| %>
  <tr>
    <td><%= chemical.name %></td>
    <td><%= chemical.casnum %></td>
    <td><%= chemical.location %></td>
    <td><%= chemical.quantity %></td>
    <td><%= link_to 'Show', chemical %></td>
    <td><%= link_to 'Edit', edit_chemical_path(chemical) %></td>
    <td><%= link_to 'Destroy', chemical, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Chemical', new_chemical_path %>
